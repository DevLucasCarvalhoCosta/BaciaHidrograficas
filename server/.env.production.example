# Porta diferente do PatrimônioUEG (que usa 3000)
PORT=3001

# Base URL da API da ANA
ANA_BASE_URL=https://dadosabertos.ana.gov.br

# Database PostgreSQL (banco separado para não conflitar)
# IMPORTANTE: Substituir SENHA_DO_POSTGRES pela senha real
DATABASE_URL="postgresql://postgres:SENHA_DO_POSTGRES@localhost:5432/ana_hidro"

# Node Environment
NODE_ENV=production

# Timeout para requisições à API da ANA (em ms)
ANA_REQUEST_TIMEOUT=60000
